{
  "name": "hoonyc",
  "description": "hoonyc is a collection of commands that hoony uses",
  "version": "0.7.0",
  "author": "the6thm0nth @the6thm0nth",
  "bin": {
    "hnc": "./bin/run"
  },
  "bugs": "https://github.com/the6thm0nth/hoonyc/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.1",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "ssh2": "^0.6.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/ssh2": "^0.5.35",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^12",
    "ts-node": "^6",
    "tslint": "^5",
    "typescript": "^2.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/the6thm0nth/hoonyc",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "hnc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/the6thm0nth/hoonyc",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
